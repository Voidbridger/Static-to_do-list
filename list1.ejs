<!DOCTYPE html>
<html>
    <head>
        <title>TODO list </title>
        <link rel="stylesheet" href="css/styles.css">
    </head>
    <body>
        <div class="box">
            <h1  id="heading">Todo List</h1>
        </div>
        <div class="box">
            <% for(var i=0 ; i< ele.length ; i++){ %>
                <div class="item">
                    <input type="checkbox" >
                    <p><%= ele[i] %> </p>

                </div>
                
            <% } %>
        <form action="/" method="POST">
            <input type="text" name="elel" placeholder="Enter the task">
            <button type="submit">+</button>
            
        </form>
        </div>
   
    </body>
</html>